{
    "presets": [
        "es2015",
        "stage-0",
        "react"
    ],
    "plugins": [
        [
            "syntax-dynamic-import"
        ],
        [
            "transform-decorators-legacy"
        ],
        [
            "transform-runtime", 
            {
                "polyfill": false,
                "regenerator": true
            }
        ],
        [
            "import",
            [
                {
                    "libraryName": "antd",
                    "style": true
                },
                { 
                    "libraryName": "antd-mobile",
                    "style": true
                },
                {
                    "libraryName": "wya-rc",
                    "libraryDirectory": "lib/web"
                }
            ]
        ]
        
    ]
}
